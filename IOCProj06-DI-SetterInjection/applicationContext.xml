<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="product1" class="com.nt.Product">
		<property name="id" value="1"></property>
		<property name="productName" value="iPhone"></property>
		<property name="productPrice" value="50000"></property>
	</bean>
	<bean id="product2" class="com.nt.Product">
		<property name="id" value="2"></property>
		<property name="productName" value="Samsung"></property>
		<property name="productPrice" value="80000"></property>
	</bean>
	<bean id="product3" class="com.nt.Product">
		<property name="id" value="3"></property>
		<property name="productName" value="Pixel"></property>
		<property name="productPrice" value="70000"></property>
	</bean>
	
	 
	<bean id="order1" class="com.nt.Order">
		<property name="emailId" value="asdf@asdf.com"></property>
		<property name="totalAmount" value="300.00"></property>
		<property name="isOrderSuccess" value="true"></property>
		<property name="foodType" value="N"></property>
		<property name="mobileNo" value="1234567890"></property>
	</bean>
	<bean id="order2" class="com.nt.Order">
		<property name="emailId" value="sdfdsfa@asdf.com"></property>
		<property name="totalAmount" value="200.00"></property>
		<property name="isOrderSuccess" value="false"></property>
		<property name="foodType" value="V"></property>
		<property name="mobileNo" value="1237457890"></property>
	</bean>

</beans>